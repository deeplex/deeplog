{
    "$schema": "https://github.com/microsoft/vcpkg-tool/raw/main/docs/vcpkg.schema.json",
    "name": "deeplog",
    "version-semver": "0.0.0-alpha.1",
    "homepage": "https://github.com/deeplex/deeplog",
    "dependencies": [
        "boost-config",
        "boost-predef",
        "boost-mp11",
        "concrete",
        "deeppack",
